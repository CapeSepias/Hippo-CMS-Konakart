<?xml version="1.0" encoding="UTF-8"?>
<sv:node sv:name="konakart-addon" xmlns:sv="http://www.jcp.org/jcr/sv/1.0">
  <sv:property sv:name="jcr:primaryType" sv:type="Name">
    <sv:value>frontend:plugincluster</sv:value>
  </sv:property>
  <sv:node sv:name="root">
    <sv:property sv:name="jcr:primaryType" sv:type="Name">
      <sv:value>frontend:plugin</sv:value>
    </sv:property>
    <sv:property sv:name="perspective.title" sv:type="String">
      <sv:value>konakart-perspective-title</sv:value>
    </sv:property>
    <sv:property sv:name="plugin.class" sv:type="String">
      <sv:value>org.onehippo.forge.konakart.cms.perspective.KonakartPerspective</sv:value>
    </sv:property>
    <sv:property sv:name="translator.id" sv:type="String">
      <sv:value>service.translator.config</sv:value>
    </sv:property>
    <sv:property sv:name="wicket.id" sv:type="String">
      <sv:value>service.tab</sv:value>
    </sv:property>
    <sv:node sv:name="layout.wireframe">
      <sv:property sv:name="jcr:primaryType" sv:type="Name">
        <sv:value>frontend:pluginconfig</sv:value>
      </sv:property>
      <sv:property sv:name="center" sv:type="String">
        <sv:value>id=hippo-panel-plugin-perspective-center,body=hippo-panel-plugin-perspective-center-body,scroll=true
        </sv:value>
      </sv:property>
      <sv:property sv:name="linked.with.parent" sv:type="String">
        <sv:value>true</sv:value>
      </sv:property>
      <sv:property sv:name="root.id" sv:type="String">
        <sv:value>hippo-panel-plugin-wrapper</sv:value>
      </sv:property>
      <sv:property sv:name="top" sv:type="String">
        <sv:value>id=hippo-panel-plugin-perspective-top,body=hippo-panel-plugin-perspective-top-body,height=60
        </sv:value>
      </sv:property>
    </sv:node>
  </sv:node>
  <sv:node sv:name="admin">
    <sv:property sv:name="jcr:primaryType" sv:type="Name">
      <sv:value>frontend:plugin</sv:value>
    </sv:property>
    <sv:property sv:name="konakart-admin-url" sv:type="String">
      <sv:value>http://localhost:8780/konakartadmin/</sv:value>
    </sv:property>
    <sv:property sv:name="plugin.class" sv:type="String">
      <sv:value>org.onehippo.forge.konakart.cms.perspective.KonakartAdminPanelPlugin</sv:value>
    </sv:property>
